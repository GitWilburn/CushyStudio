@top File { expression+ }

expression[@isGroup=Expression] {
  Identifier |
  Number |
  String |
  Permutations |
  WeightedExpression
}

@skip { space }

WeightedExpression { "(" expression+ (":" Number+)? ")" }
Permutations { "{" expression+ "}" }

@tokens {
  space { @whitespace+ }
  Identifier { $[A-Za-z_]+ }
  Number { $[0-9]+ }
  String { '"' !["]* '"' }
  // WeightedExpression {"{}"}
}